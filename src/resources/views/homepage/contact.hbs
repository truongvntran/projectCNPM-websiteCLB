<div class="main">
            <div class="left">
                <div class="card">
                    <p class=""><strong>THÔNG TIN LIÊN HỆ</strong></p>
                </div>
            </div>
            <div class="right">
                <div class="content">
                    <h1>CÂU LẠC BỘ SINH VIÊN NGHIÊN CỨU KHOA HỌC PIONEER</h1>
                    {{#each contact}}
                    <table id="customers" style="width:100%; font-size: 15px;">
                      <tr>
                        <td style="width:15%">Địa chỉ:</td>
                        <td>{{this.address}}</td>
                      </tr>
                      <tr>
                        <td>Số điện thoại</td>
                        <td>{{this.phone}}</td>
                      </tr>
                      <tr>
                        <td>Email</td>
                        <td>{{this.email}}</td>
                      </tr>
                    </table>
                    {{/each}}
                </div>
            </div>
</div>